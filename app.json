{
  "expo": {
    "name": "Save Food",
    "slug": "save-food",
    "description": "This application is created to help you saving food.",
    "githubUrl": "https://github.com/webstrongteam/save-food",
    "privacy": "public",
    "version": "1.0.0",
    "sdkVersion": "37.0.0",
    "ios": {
      "buildNumber": "1.0.1",
      "supportsTablet": false,
      "bundleIdentifier": "com.webStrong.saveFood",
