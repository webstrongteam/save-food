		"githubUrl": "https://github.com/webstrongteam/save-food",
		"privacy": "public",
		"version": "1.1.0",
		"ios": {
			"buildNumber": "1.1.0",
			"bundleIdentifier": "com.webStrong.saveFood",
			"supportsTablet": false,
			"googleServicesFile": "./firebase/GoogleService-Info.plist",
